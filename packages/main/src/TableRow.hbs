<div {{> controlData}}
	class="{{classes.main}}"
	style="{{styles.main}}"
	tabindex="{{ctr._tabIndex}}"
>
	{{#each visibleCells}}
		<div class="{{../classes.cellWrapper}}">
            {{> renderChild this}}
		</div>
	{{/each}}

	{{#each popinCells}}
		<div class="{{../classes.popin}}"
			style="grid-column-end: {{../visibleColumnLength}}" >

			<span class="{{../classes.popinTitle}}">{{this.popinText}}</span>
			<div>
                {{> renderChild this.cell}}
			</div>
		</div>
	{{/each}}

</div>
